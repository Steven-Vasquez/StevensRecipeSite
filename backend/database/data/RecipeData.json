            "recipe_instructions": [
                {
                    "step_title": "Prep Work",
                    "steps": [
                        { "step_number": 1, "instruction": "Cook the bacon in a 4.5-quart soup pot slowly over low heat. Set aside on a paper towel-lined plate. Reserve 2 tbsp of bacon drippings.", "step_notes": "", "step_images": [] }
